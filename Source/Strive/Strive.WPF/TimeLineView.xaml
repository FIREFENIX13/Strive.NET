<ad:DockableContent x:Class="Strive.WPF.TimeLineView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:ad="clr-namespace:AvalonDock;assembly=AvalonDock"
        Title="Time Line" Height="300" Width="300">
    <Slider Name="slider1" Minimum="{Binding MinVersion}" Maximum="{Binding MaxVersion}" Value="{Binding CurrentVersion, Mode=TwoWay}" />
</ad:DockableContent>
